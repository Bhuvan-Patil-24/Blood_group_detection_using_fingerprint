 {
    "functions": {
    "api/index.py": {
      "runtime": "python3.10"
    }
  },
    "version": 2,
     "builds": [
         {
             "src": "api/main.py",
             "use": "@vercel/python"
         }
     ],
     "routes": [
         {
             "src": "/(.*)",
             "dest": "api/main.py"
         }
     ]
 }
